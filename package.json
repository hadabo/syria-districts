{
  "name": "syria-districts",
  "version": "0.0.0-semantically-released",
  "description": "Get all the districts names of Syria or a random one.",
  "main": "dist/index.js",
  "scripts": {
    "precommit": "npm run test && npm run check-coverage",
    "check-coverage": "babel-istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "git-cz": "git-cz",
    "lint": "eslint --fix src/*.js",
    "test": "babel-istanbul cover -x *.test.js _mocha -- -R spec src/index.test.js",
    "prebuild": "rimraf dist",
    "build": "npm-run-all --parallel build:*",
    "build:main": "babel --copy-files --out-dir dist --ignore *.test.js src",
    "build:umd": "webpack --output-filename index.umd.js",
    "build:umd.min": "webpack --output-filename index.umd.min.js -p",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hadabo/syria-districts.git"
  },
  "keywords": [
    "random",
    "syria",
    "districts"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Abdul-hadi Hawari <hadabo@gmail.com> (http://hadabo.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hadabo/syria-districts/issues"
  },
  "homepage": "https://github.com/hadabo/syria-districts#readme",
  "dependencies": {
    "unique-random-array": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-istanbul": "0.12.2",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.1",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "codecov.io": "0.1.6",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "4.16.0",
    "eslint-config-standard": "11.0.0-beta.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-node": "5.2.1",
    "eslint-plugin-promise": "3.7.0",
    "eslint-plugin-standard": "3.0.1",
    "husky": "0.15.0-rc.2",
    "mocha": "5.0.0",
    "npm-run-all": "4.1.2",
    "rimraf": "2.6.2",
    "semantic-release": "12.2.4",
    "webpack": "3.10.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
}
